# name: test/sql/update/update_not_null.test
# description: test updating a table with a NOT NULL constraint
# group: [update]

require ducklake

require parquet

test-env DUCKLAKE_CONNECTION __TEST_DIR__/

test-env DUCKLAKE_CATALOG_DB ducklake_update_not_null.db

statement ok
ATTACH 'ducklake:${DUCKLAKE_CONNECTION}${DUCKLAKE_CATALOG_DB}' AS ducklake (DATA_PATH '__TEST_DIR__/ducklake_update_not_null_files')

statement ok
CREATE TABLE ducklake.test(i INTEGER NOT NULL, j INTEGER);

statement ok
INSERT INTO ducklake.test VALUES (42, NULL);

statement ok
BEGIN

statement error
UPDATE ducklake.test SET i=NULL
----
NOT NULL constraint failed

statement error
UPDATE ducklake.test SET i=100
----
Current transaction is aborted

statement ok
ROLLBACK

query II
FROM ducklake.test
----
42	NULL
