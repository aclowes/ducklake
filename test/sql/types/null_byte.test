# name: test/sql/types/null_byte.test
# description: Test null bytes in strings
# group: [types]

require ducklake

require parquet

test-env DUCKLAKE_CONNECTION __TEST_DIR__/

test-env DUCKLAKE_CATALOG_DB ducklake_null_byte.db

statement ok
ATTACH 'ducklake:${DUCKLAKE_CONNECTION}${DUCKLAKE_CATALOG_DB}' AS ducklake (DATA_PATH '__TEST_DIR__/ducklake_null_byte', METADATA_CATALOG 'ducklake_meta')

statement ok
CREATE TABLE ducklake.tbl(s VARCHAR);

statement ok
INSERT INTO ducklake.tbl VALUES ('goo' || chr(0) || 'se'), ('hello');

query I
FROM ducklake.tbl
----
goo\0se
hello

query I
FROM ducklake.tbl WHERE s < 'hello'
----
goo\0se
