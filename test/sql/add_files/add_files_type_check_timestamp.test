statement error
CALL ducklake_add_data_files('ducklake', 'test', '__TEST_DIR__/date.parquet')
----
DATE

statement ok
CALL ducklake_add_data_files('ducklake', 'test', '__TEST_DIR__/time.parquet')

query I
FROM ducklake.test
----
20:12:24

