# name: test/sql/constraints/unsupported.test
# description: test unsupported constraints
# group: [constraints]

require ducklake

require parquet

test-env DUCKLAKE_CONNECTION __TEST_DIR__/

test-env DUCKLAKE_CATALOG_DB ducklake_unsupported.db

statement ok
ATTACH 'ducklake:${DUCKLAKE_CONNECTION}${DUCKLAKE_CATALOG_DB}' AS ducklake (DATA_PATH '__TEST_DIR__/ducklake_unsupported_files')

statement error
CREATE TABLE ducklake.test(i INTEGER PRIMARY KEY, j INTEGER);
----
not supported

statement error
CREATE TABLE ducklake.test(i INTEGER, j INTEGER,  CHECK (i > j));
----
not supported
